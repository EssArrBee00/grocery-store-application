<UserControl x:Class="Assignment02.Views.startupPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Assignment02.Views"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="800">
    <Grid Background="AntiqueWhite">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Name="AdminButton" Command="{Binding adminCmd}" Content="Admin" Width="200" Height="50" Margin="0,0,0,40" Foreground="RosyBrown" FontSize="30" Background="White" FontWeight="Medium"></Button>
            <Button Name="CustomerButton" Command="{Binding customerCmd}" Content="Customer" Width="200" Height="50" Margin="0,40,0,0" Foreground="RosyBrown" FontSize="30" Background="White" FontWeight="Medium"></Button>
        </StackPanel>

    </Grid>
</UserControl>
